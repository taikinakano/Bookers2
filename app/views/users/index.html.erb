<!--<h1>users画面</h1>-->
<%= render 'users/info', user: @user %>
<%= render 'books/list', book: @book %>
<%= @users.each do |user| %>
<div>
    <h4>image</h4>
    <p><%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: 'no_image.jpg' %></p> <!--5章記述-->
    <p>name:<%= user.name %></p>
    <%= link_to "show" , user_path(user.id) %>
</div>
<% end %>